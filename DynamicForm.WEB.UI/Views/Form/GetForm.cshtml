@model DynamicForm.Model.Base.BaseModel.FormModel
@{
    ViewBag.Title = "GetForm";
}

<form style="margin-top:5%;" method="post" >
    @foreach (var item in Model.fieldModels)
    {
    <div class="form-group">
        <label for="FormName">@item.name</label>
        @if (item.required)
        {
            <input type="@item.dataType" name="@item.name" class="form-control" required='required' />
        }
        else
        {
            <input type="@item.dataType" name="@item.name" class="form-control" />
        }

    </div> 
    } 
    <div class="form-group">
       <button type="submit">Save</button>
    </div>
</form>
 